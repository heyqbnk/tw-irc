{
  "name": "tw-irc",
  "version": "3.0.9",
  "description": "TypeScript library for working with Twitch IRC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/wolframdeus/tw-irc.git",
  "author": "Wolfram Deus <wolfram.deus@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "tmi",
    "twitch",
    "twitch.tv",
    "stream",
    "broadcast",
    "irc",
    "message",
    "messaging",
    "interface",
    "typescript",
    "subscriber",
    "websocket",
    "ws",
    "bot",
    "robot",
    "chat"
  ],
  "scripts": {
    "preversion": "yarn test && yarn lint",
    "prepublishOnly": "yarn build",
    "build": "tsc -p tsconfig-build.json",
    "lint": "eslint --fix ./lib/**/*.ts",
    "dev": "webpack-dev-server",
    "test": "jest --config jest.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn lint && yarn build"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.15",
    "@types/jest-each": "^0.3.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-each": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.7.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {}
}
